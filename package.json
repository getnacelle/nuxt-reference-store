{
  "name": "nuxt-starter",
  "version": "0.0.2",
  "private": true,
  "engines": {
    "node": ">=14.7"
  },
  "scripts": {
    "dev": "nuxt",
    "generate": "nuxt generate",
    "serve": "serve dist -p 4000",
