  },
  "dependencies": {
    "@nacelle/vue": "^0.0.3",
    "@nuxtjs/composition-api": "^0.29.0",
    "@tailwindcss/aspect-ratio": "^0.2.2",
    "core-js": "^3.17.3",
    "isomorphic-unfetch": "^3.1.0",
    "lru-cache": "^6.0.0",
    "nuxt": "^2.15.7",
    "pascal-case": "^3.1.2"
  },
  "devDependencies": {
    "@nuxt/image": "^0.6.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/svg": "^0.3.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "fibers": "^5.0.0",
    "node-sass": "^6.0.1",
    "sass-loader": "^10.2.0",
    "semantic-release": "^18.0.0",
    "semantic-release-github-pr": "^6.0.1",
    "serve": "^12.0.1"
  },
  "release": {
    "baseBranch": "main",
    "branches": [
      "main"
